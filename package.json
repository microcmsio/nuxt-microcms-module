{
  "name": "nuxt-microcms-module",
  "version": "3.0.0",
  "description": "microCMS integration for Nuxt.js",
  "keywords": [
    "nuxt",
    "module",
    "nuxt-module",
    "microcms",
    "headless",
    "cms"
  ],
  "homepage": "https://github.com/microcmsio/nuxt-microcms-module#readme",
  "bugs": {
    "url": "https://github.com/microcmsio/nuxt-microcms-module/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/microcmsio/nuxt-microcms-module.git"
  },
  "license": "Apache-2.0",
  "author": "shibe97",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "lint": "eslint .",
    "prepack": "nuxt-module-build",
    "release": "nuxi prepare playground && yarn lint && yarn prepack && npm publish"
  },
  "dependencies": {
    "@nuxt/kit": "^3.2.0",
    "defu": "^6.1.2",
    "microcms-js-sdk": "^2.3.2"
  },
  "devDependencies": {
    "@nuxt/eslint-config": "^0.1.1",
    "@nuxt/module-builder": "^0.2.1",
    "@nuxt/schema": "^3.2.3",
    "@nuxt/test-utils": "^3.2.3",
    "@types/node": "16.18.13",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.6.0",
    "nuxt": "^3.2.3"
  }
}
